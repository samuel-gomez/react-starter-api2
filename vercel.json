{
  "version": 2,
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1"
    }
  },
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/next"
    },
    { "src": "./public/*.*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
